<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <title>Challenge 5</title>
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
    <div class="d-flex flex-row">
        <div class="sidebar d-flex flex-row">
            <div class="d-flex flex-column flex-shrink-0 text-center" style="width: 70px; background: #0D28A6;">
                <img src="/img/icon1.png" alt="icon1" style="width: 34px; height: 34px; margin: 18px 18px;">
                <div style="background: rgba(255, 255, 255, 0.3);">
                    <a href="/" class="text-decoration-none">
                        <img src="/img/fi_home.svg" alt="fi_home" class="img-icon"><p class="text-header">Dashboard</p>
                    </a>
                </div>
                <div>
                    <a href="/cars" class="text-decoration-none">
                        <img src="/img/fi_truck.svg" alt="fi_truck" class="img-icon"><p class="text-header">Cars</p>
                    </a>
                </div>
            </div>
            <div class="d-flex flex-column flex-shrink-0 bg-white" style="width: 220px;">
                <img src="/img/icon2.png" alt="icon2" style="width: 100px; height: 34px; margin: 20px 22px;">
                <div class="box-list ps-4 mt-1 pt-3" style="background: #CFD4ED;">
                    <a href="/" class="text-decoration-none text-body"><p class="text-active">Cars</p></a>
                </div>
                <div class="box-list ps-4 pt-3 mt-3">
                    <a href="/cars" class="text-decoration-none text-body"><p class="text-sidebar">List Car</p></a>
                </div>
            </div>
        </div>
        <div class="content w-100">
            <%- include('partials/navbar') %>
            <div class="main">
                <div class="pt-4 mx-4">
                    <div aria-label="breadcrumb">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item text-page"><a href="/" class="text-decoration-none text-body">Cars</a></li>
                            <li class="breadcrumb-item active text-page" aria-current="step">List Cars</li>
                        </ol>
                    </div>
                    <div class="d-flex flex-row justify-content-between mt-4 mb-3">
                        <h3>Cars</h3>
                        <a href="/cars/addCar" class="text-decoration-none">
                            <button type="button" class="btn-add d-flex justify-content-between">
                                <img src="/img/fi_plus.svg" alt="">Add New Car
                            </button>
                        </a>
                    </div>
                </div>
                <div class="d-flex flex-row ms-4">
                    <!-- <div class="box-page-active">All</div>
                    <div class="box-page ms-3">Small</div>
                    <div class="box-page ms-3">Medium</div>
                    <div class="box-page ms-3">Large</div> -->
                </div>
                <div class="container-fluid p-4 d-flex flex-row flex-wrap">
                    <div class="card p-3 me-4 mb-4" style="width: max-content;">
                        <img class="card-img-top p-4 mt-3" src="/img/cars.png" alt="cars">
                        <div class="card-body">
                            <h5 class="card-title">Nama/Tipe Mobil</h5>
                            <h3 class="card-text">Harga Sewa</h3>
                            <p><img src="/img/fi_clock.svg" alt="fi_clock"> update</p>
                            <button type="button" class="btn-car btn-delete me-2"><img src="/img/fi_trash.svg" alt="fi_trash"> Delete</button>
                            <button type="button" class="btn-car btn-edit"><img src="/img/fi_edit.svg" alt="fi_edit"> Edit</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>